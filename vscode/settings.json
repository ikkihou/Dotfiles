{
	"RainbowBrackets.depreciation-notice": false,
	"terminal.integrated.inheritEnv": false,
	"editor.fontFamily": "MesloLGM Nerd Font, FiraCode Nerd Font Propo, JetBrainsMonoNL Nerd Font Propo, Monaspace Neon, Monaco, Menlo",
	"terminal.integrated.fontSize": 16,
	"editor.fontWeight": "511",
	"terminal.integrated.fontFamily": "MesloLGM Nerd Font, FiraCode Nerd Font, JetBrainsMonoNL Nerd Font",
	"[python]": {
		"editor.defaultFormatter": "ms-python.black-formatter"
	},
	"vim.leader": " ",
	"vim.enableNeovim": true,
	"vim.smartRelativeLine": true,
	"vim.camelCaseMotion.enable": true,
	"vim.highlightedyank.enable": true,
	"vim.highlightedyank.color": "rgba(261, 240, 170, 0.5)",
	"vim.highlightedyank.textColor": "white",
	"vim.easymotion": true,
	"vim.foldfix": true,
	"vim.sneak": true,
	"vim.surround": true,
	"vim.neovimConfigPath": "/Users/baoyihui/.config/nvim/init.lua",
	"vim.neovimPath": "/opt/homebrew/bin/nvim",
	"vim.neovimUseConfigFile": true,
	"vim.useCtrlKeys": true,
	"vim.useSystemClipboard": true,
	"vim.handleKeys": {
		"<C-d>": true,
		"<C-s>": true,
		"<C-z>": true,
		"<C-a>": true
	},
	"vim.hlsearch": true,
	"vim.incsearch": true,
	"vim.insertModeKeyBindingsNonRecursive": [],
	"vim.normalModeKeyBindings": [
		{
			"before": [
				"<leader>",
				"f",
				"m"
			],
			"commands": [
				{
					"command": "editor.action.formatDocument",
					"when": "eitorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor",
					"args": []
				}
			]
		},
		{
			"before": [
				"<leader>",
				"f",
				"n"
			],
			"commands": [
				{
					"command": "notebook.formatCell",
					"when": "editorHasDocumentFormattingProvider && editorTextFocus && inCompositeEditor && notebookEditable && !editorReadonly && activeEditor == 'workbench.editor.notebook'",
					"args": []
				}
			]
		},
	],
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": [
				"<leader>",
				"g",
				"q"
			],
			"commands": [
				"workbench.action.closeEditorsInGroup"
			]
		},
		{
			"before": [
				"g",
				"r"
			],
			"commands": [
				"editor.action.rename"
			]
		},
		{
			"before": [
				"leader",
				"w"
			],
			"commands": [
				":w!"
			]
		},
		{
			"before": [
				"leader",
				"q"
			],
			"commands": [
				":q!"
			]
		},
		{
			"after": [
				"g",
				"g",
				"V",
				"G"
			],
			"before": [
				"<c-a>"
			]
		},
		{
			"before": [
				"<leader>",
				"k"
			],
			"commands": [
				"editor.action.showHover"
			]
		},
		{
			"before": [
				"[",
				"d"
			],
			"commands": [
				"editor.action.marker.prev"
			]
		},
		{
			"before": [
				"]",
				"d"
			],
			"commands": [
				"editor.action.marker.next"
			]
		},
		{
			"before": [
				"<leader>",
				"c",
				"a"
			],
			"commands": [
				"editor.action.quickFix"
			]
		},
		{
			"after": [
				"^"
			],
			"before": [
				"H"
			]
		},
		{
			"after": [
				"$"
			],
			"before": [
				"L"
			]
		},
		{
			"before": [
				"leader",
				"i"
			],
			"commands": [
				"extension.toggleBool"
			]
		}
	],
	"vim.insertModeKeyBindings": [
		{
			"before": [
				"j",
				"j"
			],
			"after": [
				"<Escape>"
			]
		},
		{
			"before": [
				"j",
				"k"
			],
			"after": [
				"<Escape>"
			]
		}
	],
	"files.autoSave": "afterDelay",
	"editor.fontSize": 15,
	"jupyter.askForKernelRestart": false,
	"editor.indentSize": "tabSize",
	"editor.minimap.enabled": false,
	"catppuccin.extraBordersEnabled": true,
	"cmake.configureOnOpen": true,
	"explorer.confirmDelete": false,
	"[cpp]": {
		"editor.defaultFormatter": "xaver.clang-format"
	},
	"C_Cpp.default.cppStandard": "c++28",
	"C_Cpp.formatting": "clangFormat",
	"C_Cpp.clang_format_path": "/opt/homebrew/bin/clang-format",
	"cmake.cmakePath": "/opt/homebrew/bin/cmake",
	"cmake.additionalCompilerSearchDirs": [
		"/usr/bin/"
	],
	"cmake.debugConfig": {
		"miDebuggerPath": "/Users/baoyihui/.vscode/extensions/ms-vscode.cpptools10.16.3-darwin-arm64/debugAdapters/lldb-mi/bin/lldb-mi"
	},
	"indentRainbow.indentSetter": {},
	"RainbowBrackets.showBracketsInRuler": true,
	"editor.wordWrap": "on",
	"explorer.confirmDragAndDrop": false,
	"editor.codeActionsOnSave": {
		"source.fixAll": "explicit"
	},
	"editor.formatOnSave": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"clang-format.fallbackStyle": "Google",
	"C_Cpp.intelliSenseEngine": "disabled",
	"window.commandCenter": false,
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"remote.SSH.defaultForwardedPorts": [],
	"remote.SSH.showLoginTerminal": true,
	"remote.SSH.configFile": "/Users/baoyihui/.ssh/config",
	"git.ignoreLegacyWarning": true,
	"git.openRepositoryInParentFolders": "never",
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.smoothScrolling": true,
	"terminal.integrated.smoothScrolling": true,
	"workbench.list.smoothScrolling": true,
	"lldb.verboseLogging": true,
	"lldb.dbgconfig": {},
	"editor.accessibilitySupport": "off",
	"clangd.path": "/usr/bin/clangd",
	"[cmake]": {
		"editor.defaultFormatter": "cheshirekow.cmake-format"
	},
	"terminal.integrated.enableMultiLinePasteWarning": false,
	"jupyter.widgetScriptSources": [
		"jsdelivr.com",
		"unpkg.com"
	],
	"workbench.editorAssociations": {
		"*.py": "default"
	},
	"todohighlight.isEnable": true,
	"editor.fontVariations": true,
	"workbench.iconTheme": "catppuccin-macchiato",
	"vsicons.presets.hideExplorerArrows": true,
	"material-icon-theme.hidesExplorerArrows": false,
	"workbench.colorTheme": "Catppuccin Macchiato",
	"gruvboxMaterial.darkCursor": "red",
	"gruvboxMaterial.darkSelection": "yellow",
	"gruvboxMaterial.italicKeywords": true,
	"gruvboxMaterial.diagnosticTextBackgroundOpacity": "23.5%",
	"gruvboxMaterial.highContrast": true,
	"gruvboxMaterial.darkContrast": "soft",
	"gruvboxMaterial.darkWorkbench": "flat",
	"gruvboxMaterial.colorfulSyntax": true,
	"git.confirmSync": false,
	"everforest.highContrast": true,
	"everforest.italicKeywords": true,
	"everforest.darkWorkbench": "high-contrast",
	"gitProjectManager.displayProjectPath": true,
	"projectManager.git.baseFolders": [
		"/Users/baoyihui/Documents/GitHub/",
	],
	"notebook.lineNumbers": "on",
	"[lua]": {
		"editor.defaultFormatter": "yinfei.luahelper"
	},
	"rust-analyzer.debug.engine": "vadimcn.vscode-lldb",
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.cursorBlinking": "smooth",
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"python.testing.pytestEnabled": true,
	"python.testing.pytestArgs": [
		"-s",
		"-v",
		"--capture=no",
		"--log-cli-level=DEBUG",
	],
	"python.testing.cwd": "${workspaceFolder}/tests",
	"python.testing.autoTestDiscoverOnSaveEnabled": true,
	"python.analysis.inlayHints.pytestParameters": true,
	"todohighlight.keywords": [
		{
			"text": "NOTE:",
			"color": "white",
			"backgroundColor": "green",
			"overviewRulerColor": "grey",
			"border": "1px solid green",
			"borderRadius": "2px",
		},
	],
	"python.terminal.focusAfterLaunch": true,
	"editor.suggest.localityBonus": true,
	"python.analysis.completeFunctionParens": true,
	"python.languageServer": "Pylance",
	"git.suggestSmartCommit": false,
	"latex-workshop.linting.chktex.enabled": false,
	"latex-workshop.linting.chktex.exec.path": "/Library/TeX/texbin/chktex",
	"latex-workshop.latex.autoBuild.run": "never",
	"latex-workshop.showContextMenu": true,
	"latex-workshop.intellisense.package.enabled": true,
	"latex-workshop.message.error.show": false,
	"latex-workshop.message.warning.show": false,
	"latex-workshop.latex.tools": [
		{
			"name": "xelatex",
			"command": "xelatex",
			"args": [
				"-synctex=1",
				"-interaction=nonstopmode",
				"-file-line-error",
				"%DOCFILE%"
			]
		},
		{
			"name": "pdflatex",
			"command": "pdflatex",
			"args": [
				"-synctex=1",
				"-interaction=nonstopmode",
				"-file-line-error",
				"%DOCFILE%"
			]
		},
		{
			"name": "latexmk",
			"command": "latexmk",
			"args": [
				"-synctex=1",
				"-interaction=nonstopmode",
				"-file-line-error",
				"-pdf",
				"-outdir=%OUTDIR%",
				"%DOCFILE%"
			]
		},
		{
			"name": "bibtex",
			"command": "bibtex",
			"args": [
				"%DOCFILE%"
			]
		}
	],
	"latex-workshop.latex.recipes": [
		{
			"name": "XeLaTeX",
			"tools": [
				"xelatex"
			]
		},
		{
			"name": "PDFLaTeX",
			"tools": [
				"pdflatex"
			]
		},
		{
			"name": "BibTeX",
			"tools": [
				"bibtex"
			]
		},
		{
			"name": "LaTeXmk",
			"tools": [
				"latexmk"
			]
		},
		{
			"name": "xelatex -> bibtex -> xelatex*2",
			"tools": [
				"xelatex",
				"bibtex",
				"xelatex",
				"xelatex"
			]
		},
		{
			"name": "pdflatex -> bibtex -> pdflatex*2",
			"tools": [
				"pdflatex",
				"bibtex",
				"pdflatex",
				"pdflatex"
			]
		},
	],
	"latex-workshop.latex.clean.fileTypes": [
		"*.aux",
		"*.bbl",
		"*.blg",
		"*.idx",
		"*.ind",
		"*.lof",
		"*.lot",
		"*.out",
		"*.toc",
		"*.acn",
		"*.acr",
		"*.alg",
		"*.glg",
		"*.glo",
		"*.gls",
		"*.ist",
		"*.fls",
		"*.log",
		"*.fdb_latexmk"
	],
	"latex-workshop.latex.autoClean.run": "onFailed",
	"latex-workshop.latex.recipe.default": "lastUsed",
	"latex-workshop.view.pdf.internal.synctex.keybinding": "double-click",
	"[latex]": {
		"editor.defaultFormatter": "James-Yu.latex-workshop"
	},
	"window.zoomLevel": -1
}